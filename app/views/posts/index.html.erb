<h1>Listing posts1111</h1>

<%= link_to 'Nuevo Post', new_post_path %>

<table>
  <tr>
    <th>ID</th>
    <th>Title</th>
    <th>Text</th>
  </tr>

  <% @posts.each do |t| %>
    <tr>
      <td><%= t.id %></td>
      <td><%= t.title %></td>
      <td><%= t.text %></td>
      <td><%= link_to 'Show', post_path(t) %></td>
      <td><%= link_to 'Edit', edit_post_path(t) %></td>
<td><%= link_to 'Dar de baja' ,post_path(t), method: :delete %></td>
<td><%= link_to 'Destroy', post_path(t),
           method: :delete,
           data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>
